-- Load Rayfield UI Library
local Rayfield = loadstring(game:HttpGet("https://raw.githubusercontent.com/SiriusSoftwareLtd/Rayfield/main/source.lua"))()

-- Create Rayfield window with Key System
local Window = Rayfield:CreateWindow({
    Name = "Versal new system",
    LoadingTitle = "Loading...",
    LoadingSubtitle = "by agent duck",
    ConfigurationSaving = {
        Enabled = false
    },
    Discord = {
        Enabled = false
    },
    KeySystem = true,
    KeySettings = {
        Title = "Enter Key",
        Subtitle = "Authentication Required",
        Note = "Join versal scripts for key https://discord.gg/5KzEQKVNde",
        FileName = "Versal2025",
        SaveKey = false,
        GrabKeyFromSite = false,
        Key = { "Versal2025" }
    }
})

-- Create main tab and section
local MainTab = Window:CreateTab("Main", 4483362458)
MainTab:CreateSection("Da Hood Script")

-- Create button to run Da Hood script
MainTab:CreateButton({
    Name = "Da Hood Script",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/wild27272/da-hood/main/full%20script"))()
    end,
})

-- Create button to run G.A.G Script
MainTab:CreateButton({
    Name = "G.A.G Script",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/wild27272/grow-a-garden/main/obsucated%20v2"))()
    end,
})

-- Create button to run Rivals Script
MainTab:CreateButton({
    Name = "Rivals Script",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/wild27272/stuff-for-gui/main/rivals"))()
    end,
})
